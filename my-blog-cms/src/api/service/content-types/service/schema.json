{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service",
    "description": "Company services"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "content": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "icon": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "icon_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "gradientFrom": {
      "type": "customField",
      "customField": "plugin::color-picker.color",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "gradientTo": {
      "type": "customField",
      "customField": "plugin::color-picker.color",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "featured": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": false
    },
    "order": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": 0
    },
    "tags": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "service_video": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["videos"]
    },
    "video_url": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "plans": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::plan.plan",
      "inversedBy": "services"
    },
    "buttonLabel": {
      "type": "string",
      "maxLength": 50,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": "Learn More"
    }
  }
}
